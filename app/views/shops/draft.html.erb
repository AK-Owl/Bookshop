<% @books.each do |book| %>
<% if notice %>
<aside id="notice"><%= notice %></aside>
<% end %>
	<h1> Catalog</h1>
  <ul class = "catalog">
    <% cache @books do %>
     
        <li>
        <%= image_tag("http://covers.openlibrary.org/b/isbn/#{book.isbn}-L.jpg") %>

        <h2><%= book.title %></h2>
        <p>

        <p>
        <div class="price">
        <%= book.price %>
        <div>
        </li>
      <% end%>
    <% end %>
    </ul>